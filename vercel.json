{
    "version": 2,
    "name": "music-app-server",
    "builds": [
       { "src": "app.js", "use": "@vercel/node" }
    ],
    "routes": [
       { "src": "/(.*)", "dest": "/app.js" }
    ],
    "headers": [
      {
        "source": "/api/(.*)", 
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://music-app-client-five.vercel.app" 
          }
        ]
      }
    ]
 }